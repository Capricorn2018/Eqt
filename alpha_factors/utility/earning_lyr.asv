function [ output_args ] = earning_lyr(folder)
%UNTITLED 此处显示有关此函数的摘要
%   此处显示详细说明
    files = dir(folder);
    N = length(files);
    name = cell(N,1);
    isdir = zeros(N,1);
    
    for i=1:N
        name{i} = files(i).name;
        isdir(i) = files(i).isdir;
    end

    name = name(isdir==0);
    
    for i = 1:length(name)
        
        dt = file2dt(name);
        
        
    end
    
end


function dt = file2dt(filename)

    dt = filename(5:12);

end

